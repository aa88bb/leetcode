class Solution {
public:
    int minMoves(vector<int>& nums) {
        int count=0;
        int min=nums[0];
        for(int i=0;i<nums.size();i++){
            if(min>nums[i]){
                min=nums[i];
            }
        }
        for(int i=0;i<nums.size();i++){
            count += nums[i]-min;
        }
        return count;
    }
};
